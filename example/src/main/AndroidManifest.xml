<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.hujiaweibujidao.viewanimation"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.BounceActivity"/>
        <activity android:name=".activity.Bounce2Activity"/>
        <activity android:name=".activity.DemoActivity"/>
        <activity android:name=".activity.Demo2Activity"/>
        <activity android:name=".activity.InterpolatorActivity"/>
        <activity android:name=".activity.EvaluatorActivity"/>

    </application>

</manifest>